{
  "name": "graph-ts-gen",
  "version": "0.1.0",
  "main": "src/gen.ts",
  "license": "MIT",
  "scripts": {
      "build-msg": "protoc --plugin=protoc-gen-as=./node_modules/.bin/as-proto-gen --proto_path=../third_party/ --as_out=../graph-ts $(grep -slR 'message Msg' ../third_party/| egrep '\\.proto$') ../third_party/cosmos/authz/v1beta1/authz.proto ../third_party/cosmos/bank/v1beta1/bank.proto ../third_party/cosmos/base/v1beta1/coin.proto ../third_party/google/protobuf/any.proto ../third_party/google/protobuf/timestamp.proto ../third_party/cosmos/gov/v1beta1/gov.proto ../third_party/cosmos/staking/v1beta1/staking.proto ../third_party/ibc/core/client/v1/client.proto ../third_party/ibc/core/channel/v1/channel.proto ../third_party/ibc/core/connection/v1/connection.proto ../third_party/ibc/core/commitment/v1/commitment.proto ../third_party/confio/proofs.proto",
      "build-tendermint": "protoc --plugin=protoc-gen-as=./node_modules/.bin/as-proto-gen --proto_path=../third_party/ --as_out=../graph-ts $(find ../third_party/tendermint -type f -name '*.proto')",
      "build-google": "protoc  --plugin=protoc-gen-as=./node_modules/.bin/as-proto-gen --proto_path=../third_party/ --as_out=../graph-ts $(find ../third_party/google/ -type f -name '*.proto')",
      "build": "yarn build-msg && yarn build-tendermint && yarn build-google",
      "postbuild": "prettier --write -c ../graph-ts/**/*.ts"

  },
  "dependencies": {
    "as-proto-gen": "^0.2.3"
  }
}
